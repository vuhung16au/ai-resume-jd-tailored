{
  "build": {
    "env": {
      "DISABLE_ESLINT_PLUGIN": "true",
      "ESLINT_SKIP_DISPLAY_WARNING": "true",
      "NEXT_IGNORE_ESLINT": "true", 
      "NEXT_TELEMETRY_DISABLED": "1",
      "NEXT_IGNORE_TYPE_ERROR": "true"
    }
  },
  "buildCommand": "next build --no-lint",
  "framework": "nextjs"
}
